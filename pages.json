{
  "easycom": {
    "^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue",
    "^zshu-(.*)": "@/components/zshu-components/zshu-$1.vue",
    "^ylx-(.*)": "@/components/ylx-components/ylx-$1.vue"
  },

  "pages": [

    {
      "path": "pages/tabbar1/tabbar1",
      "style": {
        "navigationBarTitleText": "tabbar1",
        "enablePullDownRefresh": false,
        "navigationStyle": "default"
      }
    },
    {
      "path": "pages/tabbar2/tabbar2",
      "style": {
        "navigationBarTitleText": "tabbar2",
        "enablePullDownRefresh": false,
        "navigationStyle": "default"
      }
    },
    {
      "path": "pages/tabbar3/tabbar3",
      "style": {
        "navigationBarTitleText": "tabbar3",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    }
  ],
  "tabBar": {
    "selectedColor": "#FD814A",
    "color": "#999999",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/tabbar1/tabbar1",
        "text": "消息",
        "iconPath": "static/tabbar/news.png",
        "selectedIconPath": "static/tabbar/news_on.png"
      },
      {
        "pagePath": "pages/tabbar2/tabbar2",
        "text": "首页",
        "iconPath": "static/tabbar/index.png",
        "selectedIconPath": "static/tabbar/index_on.png"
      },
      {
        "pagePath": "pages/tabbar3/tabbar3",
        "text": "我的",
        "iconPath": "static/tabbar/mine.png",
        "selectedIconPath": "static/tabbar/mine_on.png"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {}
}
